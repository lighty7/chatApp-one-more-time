{
  "name": "chatterbox",
  "version": "1.0.0",
  "description": "A modern real-time chat application with Socket.io featuring multiple rooms, message history, and typing indicators",
  "private": false,
  "main": "server.js",
  "scripts": {
    "test": "mocha --exit tests/*",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "security:check": "npm audit --audit-level high"
  },
  "keywords": [
    "chat",
    "real-time",
    "socket.io",
    "websocket",
    "nodejs",
    "express"
  ],
  "author": "Qugates",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.57.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.3",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Qugates/chatterbox"
  }
}
